{
  "compilerOptions": {
      "target": "ES5",
      "module": "commonjs",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "strictPropertyInitialization": false,
  },
  "include": [
      "src/**/*.ts"
  ],
  "exclude": [
      "node_modules",
      "test",
      "**/*.spec.ts",
      "**/*.test.ts",
  ],
}